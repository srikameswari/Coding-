import java.util.*; 
class frequencies
{ 
 public static void main(String args[])
 { 
  HashMap<Integer,Integer> x=new HashMap<Integer,Integer>(); 
  int[] arr={4,4,5,5,6};
  int i; 
  for(i=0;i<arr.length;i++)
  { 
   int key;
   key=arr[i];
   if(x.containsKey(key)) 
    x.put(key,x.get(key)+1); 
   else
    x.put(arr[i],1);
  }
  System.out.println(x); 
 }
}
